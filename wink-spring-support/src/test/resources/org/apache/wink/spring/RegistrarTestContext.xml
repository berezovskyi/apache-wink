<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<bean class="org.apache.wink.spring.Registrar">
		<property name="classes">
			<set value-type="java.lang.Class">
				<value>
					org.apache.wink.spring.RegistrarTest$Resource
				</value>
				<value>
					org.apache.wink.spring.RegistrarTest$Provider
				</value>
			</set>
		</property>
		<property name="instances">
			<set>
				<ref local="resource" />
			</set>
		</property>
	</bean>

	<bean id="resource"
		class="org.apache.wink.spring.RegistrarTest$AnotherResource" />

	<!-- Used to validate that all placeholders are resolved -->
	<bean id="symphony.validatingPlaceholderConfigurer"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
	</bean>

</beans>